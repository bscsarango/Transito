<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bs.Hello_controlT" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MLControlR</name>
    <description>
       Unidad Municipal de Transporte Terrestre Transito y Seguridad Vial.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Borys Sarango Ruiz
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />

<gap:plugin name="com.phonegap.plugins.barcodescanner" />
<gap:plugin name="cordova-plugin-camera" />
<gap:plugin name="cordova-plugin-file" />
<gap:plugin name="cordova-plugin-file-transfer" />
<gap:plugin name="cordova-plugin-whitlist" />
<gap:plugin name="org.apache.cordova.device" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
